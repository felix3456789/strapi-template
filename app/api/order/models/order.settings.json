{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "amount": {
      "type": "decimal"
    },
    "created_date": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "processing",
        "sent",
        "delivery"
      ]
    },
    "shipping_detail": {
      "model": "shipping-detail",
      "via": "order_id"
    },
    "order_detail": {
      "model": "order-detail",
      "via": "order_id"
    }
  }
}
